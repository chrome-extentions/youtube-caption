{
  "manifest_version": 3,
  "name": "YouTube Caption",
  "version": "1.0",
  "description": "Show the YouTube caption.",
  "permissions": [
    "activeTab",
    "tabs",
    "storage",
    "scripting"
  ],
  "background": {
    "service_worker": "background.js"
  },
  "action": {
    "default_popup": "setting.html"
  }
}
